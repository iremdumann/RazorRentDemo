@page
@model RazorRentDemo.Pages.Rent.ReturnProcModel
@{
}
<h1>ReturnProc</h1>

<h3>Are you sure you want to return this?</h3>
<div>
    <h4>Reservation</h4>
  
    <ul>
        <li>@Model.Reservation?.Car.Brand</li>
        <li>@Model.Reservation?.Car.UnitPrice</li>
    </ul>

    <dl class="row">
        <dt class="col-sm-2">
         Customer:
        </dt>
        <dd class="col-sm-10">
            @Model.Reservation?.CustomerName
        </dd>
        <dt class="col-sm-2">
           Start:
        </dt>
        <dd class="col-sm-10">
            @Model.Reservation?.Start
        </dd>
       
        <dt class="col-sm-2">
            TotalPrice(estimated):
        </dt>
        <dd class="col-sm-10">
            @Model.Reservation?.TotalPrice
        </dd>
    </dl>

   
      
</div>



<a asp-page="Return" asp-page-handler="ReturnOK" asp-route-id="@Model.Reservation?.Id" class=" btn btn-danger">RETURN!</a>
<a asp-page-handler="Test" class="btn btn-danger">TEST!</a>
<h1>@ViewData["TESTDATAKEY"]</h1>